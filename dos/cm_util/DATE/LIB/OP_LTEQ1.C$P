/*****************************************************************************
*
*  TITLE:        operator <=
*
*  DESCRIPTION:  The subprogram "DATE::operator <="
*                in the class DATE
*                returns wether the given date
*                is less than or equal to (before or the same as)
*                the object's.
*
*  *k "%n"
*  FILE NAME:    ""
*
*  *k "%v"
*  VERSION:      ""
*
*  REFERENCE:    None.
*
*****************************************************************************/

#ifndef __DATE_CLASS
#include "date.h"
#endif


BOOLEAN DATE::operator <= (

     DATE& date)

{

   return (BOOLEAN) ((*this < date) ||
                     (*this == date));

}   // DATE::operator <=

