#ifndef __DATE_CLASS
#include "date.h"
#endif


BOOLEAN DATE::operator == (

     DATE& date)

{

   return (BOOLEAN) ((year == date.year) &&
                     (month == date.month) &&
                     (day == date.day));

}   // DATE::operator ==

